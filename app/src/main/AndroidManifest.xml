<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.blooddonorreceiver1">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sym_background"
        android:label="Blood Donor-Receiver"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BloodDonorReceiver1">
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Donor"
                  android:label="Donor Form"/>
        <activity android:name=".ShowActivity"/>
        <activity android:name=".Notification"
                  android:label="Notification"/>
        <activity android:name=".About"
                  android:label="About"/>
        <activity android:name=".Services"
                  android:label="Services"/>


    </application>

</manifest>